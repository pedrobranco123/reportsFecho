  <DataSets>
    <DataSet Name="dados">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@chave_primaria">
            <Value>=Parameters!chave_primaria.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@codprocs">
            <Value>=Parameters!codprocs.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
    COALESCE(NULLIF(acscatr.CATEGORI, ''), 'Outras') as CATEGORI,
    sum(acsspres.utente) as utente, 
    acsmovim.CODPROC7, 
    acstitul.nome, 
    sum(acsspres.valor) as valor
FROM acsmovim
left JOIN acsspres ON acsspres.codmovim = acsmovim.codmovim
left JOIN acstitul ON acstitul.codtitul = acsmovim.codtitul
left join acscatr on acsspres.codregrc = acscatr.codsrsa9
WHERE acstitul.codtitul = @chave_primaria
AND acsmovim.CODPROC7 = @codprocs
and acsmovim.movfinan = 'TXC'
group by categori, CODPROC7, nome</CommandText>
      </Query>
      <Fields>
        <Field Name="CATEGORI">
          <DataField>CATEGORI</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="utente">
          <DataField>utente</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="CODPROC7">
